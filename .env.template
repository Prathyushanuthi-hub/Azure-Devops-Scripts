# Environment Configuration Template
# Copy this file to .env and fill in your actual values

# GitHub Configuration
GITHUB_TOKEN=ghp_your_github_personal_access_token_here
GITHUB_ORG=your-github-organization-name

# Azure DevOps Configuration  
AZURE_DEVOPS_TOKEN=your_azure_devops_personal_access_token_here
AZURE_DEVOPS_ORG=your-azure-devops-organization-name
AZURE_DEVOPS_PROJECT=your-azure-devops-project-name

# Optional: API Configuration
GITHUB_API_URL=https://api.github.com
AZURE_DEVOPS_API_VERSION=6.0

# Optional: Rate Limiting
GITHUB_RATE_LIMIT_PER_HOUR=5000
AZURE_DEVOPS_RATE_LIMIT_PER_HOUR=10000